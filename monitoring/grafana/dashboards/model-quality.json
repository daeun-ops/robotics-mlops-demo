{
  "title": "Model Quality",
  "panels": [
    { "type": "stat", "title": "p95 latency (s)",
      "targets": [{ "expr": "histogram_quantile(0.95, sum(rate(inference_duration_seconds_bucket[5m])) by (le))" }] },
    { "type": "timeseries", "title": "Error rate (%)",
      "targets": [{ "expr": "100 * sum(rate(inference_failure_total[5m]))/sum(rate(inference_requests_total[5m]))" }] },
    { "type": "timeseries", "title": "Confidence drift (EMD)",
      "targets": [{ "expr": "confidence_drift" }] },
    { "type": "heatmap", "title": "Confidence histogram",
      "targets": [{ "expr": "sum(rate(inference_confidence_bucket[5m])) by (le)" }] }
  ],
  "schemaVersion": 37, "version": 1
}
